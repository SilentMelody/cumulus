{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2",
    "es2015",
    "react"
  ],
  "env": {
    "development": {

    }
  },
  "plugins": [
    "transform-runtime",
    "syntax-dynamic-import",
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": "css"
    }]
  ]
}
